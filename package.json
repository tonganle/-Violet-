{
  "name": "winjoinit",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "dll": "rimraf public/vendor && webpack -p --progress --config webpack.dll.conf.js",
    "lint": "eslint --fix --ext .js,.vue src",
    "view": "serve -s -l 8080 dist"
  },
  "dependencies": {
    "@tinymce/tinymce-vue": "^2.1.0",
    "axios": "^0.18.0",
    "babel-polyfill": "^6.26.0",
    "clipboard": "^2.0.4",
    "coordtransform": "^2.1.2",
    "core-js": "^2.6.5",
    "crypto": "^1.0.1",
    "crypto-js": "^3.1.9-1",
    "echarts": "^4.1.0",
    "element-ui": "2.9.1",
    "file-saver": "^2.0.1",
    "install": "^0.13.0",
    "jquery": "^3.5.0",
    "js-base64": "^2.4.8",
    "jspdf": "^1.5.3",
    "mockjs": "^1.0.1-beta3",
    "screenfull": "^4.0.1",
    "tinymce": "^5.0.7",
    "vue": "^2.6.10",
    "vue-awesome-swiper": "^3.1.3",
    "vue-baidu-map": "^0.21.22",
    "vue-clipboard2": "^0.3.0",
    "vue-cropper": "^0.4.9",
    "vue-i18n": "^8.11.2",
    "vue-print-nb": "^1.4.0",
    "vue-router": "^3.0.3",
    "vue-splitpane": "^1.0.4",
    "vuex": "^3.0.1",
    "xlsx": "^0.14.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.8.0",
    "@vue/cli-plugin-eslint": "^3.8.0",
    "@vue/cli-service": "^3.8.0",
    "@vue/eslint-config-standard": "^4.0.0",
    "add-asset-html-webpack-plugin": "^3.1.3",
    "babel-eslint": "^10.0.2",
    "compression-webpack-plugin": "^3.0.0",
    "eslint": "^6.1.0",
    "eslint-config-standard": "^13.0.1",
    "eslint-plugin-html": "^6.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-vue": "^5.2.3",
    "postcss": "^7.0.17",
    "rimraf": "^2.6.3",
    "sass": "^1.18.0",
    "sass-loader": "^7.1.0",
    "script-loader": "^0.7.2",
    "serve": "^11.0.2",
    "style-loader": "^0.23.1",
    "uglifyjs-webpack-plugin": "^2.1.3",
    "vue-template-compiler": "^2.6.10",
    "webpack-cli": "^3.3.5",
    "webpack-parallel-uglify-plugin": "^1.1.1"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {
      "indent": [
        2,
        2
      ],
      "quotes": [
        2,
        "single"
      ],
      "semi": [
        2,
        "never"
      ],
      "quote-props": [
        2,
        "as-needed"
      ],
      "no-unused-vars": [
        1
      ],
      "camelcase": [
        0
      ]
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
